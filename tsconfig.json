{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "lib": ["WebWorker"],
    "types": [
      "astro/client",
      "unplugin-icons/types/react",
      "vite-plugin-svgr/client",
      "vite-plugin-pwa/client"
    ],
    "skipLibCheck": true,
    "exactOptionalPropertyTypes": false,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "~/layouts/*": ["./src/components/layouts/*"],
      "~/*": ["./src/*"]
    }
  }
}
